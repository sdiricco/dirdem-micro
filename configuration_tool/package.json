    "ngx-electron": "^2.2.0",
    "python-shell": "^1.0.8",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.1",
    "webpack-target-electron-renderer": "^0.4.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
