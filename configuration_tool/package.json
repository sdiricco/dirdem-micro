{
  "name": "dirdem-micro",
  "productName": "Dirdem Micro",
  "version": "0.1.1",
  "main": "main.js",
  "author": "DM Inc.",
  "description": "The Builder",
  "legalTrademarks": "(c) 2020 dirdem.com",
  "publisherName": "dirderm.com",
  "target": [
    "portable"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build && electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . dirdem-micro --overwrite --asar --platform=win32 --arch=ia32 --icon=src/assets/logo/dirdem-micro-icon-app.ico --prune=true --out=release-builds/win-package --version-string.CompanyName=\"DM Inc.\" --version-string.FileDescription=\"The builder\" --version-string.ProductName=\"Dirdem Micro\"",
    "package-linux": "electron-packager . dirdem-micro --overwrite --asar --platform=linux --arch=x64 --icon=src/assets/logo/dirdem-micro-icon-app.png --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
